    "http://localhost:3030"
  ],
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mysql": {
    "client": "mysql",
    "connection": "mysql://root:mysql@localhost:3306/you"
  },
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "",
    "authStrategies": [
      "jwt",
      "otp"
    ],
