      "header": {
        "typ": "access"
      },
      "audience": "https://id.youcoin.org",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "otp": {
      "expriesIn": 10
    }
  },
  "appSecret":"",
  "sendgrid":{
    "apiKey": "",
    "sender": "hello@youcoin.org"
  }
}